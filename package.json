{
  "name": "catalyst-b2b-makeswift",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "predeploy": "node ./scripts/copy-b2b-dist.mjs",
    "build:storefront": "turbo run build --filter=storefront",
    "build:catalyst": "turbo run build --filter=@bigcommerce/catalyst-makeswift",
    "build": "pnpm build:storefront && pnpm predeploy && pnpm build:catalyst"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
